<p style="color: #004aad"><%= notice %></p>

<h1 class="heading">Workout Logs</h1>

<div id="workout_logs">
  <% @workout_logs.each do |workout_log| %>
    <%= render workout_log %>
    <p class="log">
      <%= link_to "Show this workout log", workout_log, class:"buttons" %>
    </p>
  <% end %>
</div>
<div class="log">
  <%= link_to "New workout log", new_workout_log_path, class:"buttons log1" %>
</div>
